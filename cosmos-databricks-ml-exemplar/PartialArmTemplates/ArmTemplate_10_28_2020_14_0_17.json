{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cosmos-databricks-ml-exemplar"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/populate_weather_cosmos')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "copy weather data",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "JsonSource",
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true,
									"wildcardFileName": "*.json"
								},
								"formatSettings": {
									"type": "JsonReadSettings"
								}
							},
							"sink": {
								"type": "CosmosDbSqlApiSink",
								"writeBehavior": "upsert",
								"disableMetricsCollection": true
							},
							"enableStaging": false,
							"parallelCopies": 10,
							"enableSkipIncompatibleRow": true,
							"dataIntegrationUnits": 128
						},
						"inputs": [
							{
								"referenceName": "source_weatherdata_json",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "exemplar_weatherdata_cosmos",
								"type": "DatasetReference"
							}
						]
					}
				],
				"folder": {
					"name": "setup"
				},
				"annotations": [],
				"lastPublishTime": "2020-10-13T03:52:07Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/source_weatherdata_json')]",
				"[concat(variables('factoryId'), '/datasets/exemplar_weatherdata_cosmos')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/exemplar_weatherdata_cosmos')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "weatherdata_exemplar",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "exemplar_sources"
				},
				"annotations": [],
				"type": "CosmosDbSqlApiCollection",
				"schema": {},
				"typeProperties": {
					"collectionName": "weatherfinal"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/source_weatherdata_json')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "holidaydata_exemplar",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "source_files"
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "*.json",
						"fileSystem": "weatherdata"
					}
				},
				"schema": {}
			},
			"dependsOn": []
		}
	]
}